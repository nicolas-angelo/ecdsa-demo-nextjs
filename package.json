    "build:icons": "npx @svgr/cli --out-dir src/assets/icons -- icons",
    "format": "prettier --write \"{src,.vscode}/**/*.{ts,tsx,json}\""
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.17",
    "axios": "^1.3.4",
    "deta": "^1.1.0",
    "ethereum-cryptography": "^1.2.0",
    "next": "^13.2.4",
    "next-auth": "^4.21.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "swr": "^2.1.2"
  },
  "devDependencies": {
    "@svgr/plugin-jsx": "^7.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "18.15.11",
    "@types/react": "18.0.31",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.6",
    "tailwindcss": "^3.2.7",
